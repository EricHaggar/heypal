{
    "name": "simple-react-full-stack",
    "version": "1.0.0",
    "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
    "main": "src/server/app.js",
    "scripts": {
        "build": "webpack --mode production",
        "start": "npm run build && node src/server/index.js",
        "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
        "server": "nodemon src/server/index.js",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "format": "npm run format:prettier && npm run format:eslint",
        "format:prettier": "prettier --write \"src/**/*.*\"",
        "format:eslint": "eslint --fix \"src/**/*.js\""
    },
    "author": "Sandeep Raveesh",
    "license": "ISC",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.22",
        "@fortawesome/free-brands-svg-icons": "^5.10.2",
        "@fortawesome/free-solid-svg-icons": "^5.10.2",
        "@fortawesome/react-fontawesome": "^0.1.4",
        "@google-cloud/language": "^3.2.6",
        "antd": "^3.23.2",
        "axios": "^0.21.1",
        "babel-polyfill": "^6.26.0",
        "bluebird": "^3.5.5",
        "body-parser": "^1.19.0",
        "chart.js": "^2.8.0",
        "express": "^4.16.3",
        "http-proxy-middleware": "^1.0.4",
        "prop-types": "^15.7.2",
        "python-shell": "^1.0.8",
        "react": "^16.5.2",
        "react-chartjs-2": "^2.7.6",
        "react-charts": "^2.0.0-alpha.12",
        "react-dom": "^16.5.2",
        "react-loader-spinner": "^3.1.4",
        "react-router-dom": "^5.2.0",
        "sentiment": "^5.0.2",
        "twitter": "^1.7.1"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^10.0.0",
        "babel-loader": "^8.0.0",
        "clean-webpack-plugin": "^1.0.0",
        "concurrently": "^5.2.0",
        "css-loader": "^2.0.0",
        "eslint": "^5.0.0",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.11.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.7.0",
        "file-loader": "^3.0.0",
        "html-webpack-plugin": "^3.2.0",
        "nodemon": "^2.0.4",
        "prettier": "2.0.5",
        "style-loader": "^0.23.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.5.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0"
    },
    "proxy": "http://localhost:8080/"
}
